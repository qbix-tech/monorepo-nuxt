{
  "name": "@org/queue",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "unbuild --stub",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "bullmq": "^5.58.1",
    "defu": "^6.1.4",
    "ioredis": "^5.7.0"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/dist"
        ]
      }
    }
  }
}
